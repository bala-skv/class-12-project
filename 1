create table GAMES(
  GCODE INT primary key,
  GAMENAME VARCHAR(15),
  eventcount INT,
  PRIZEMONEY INT NOT NULL DEFAULT 10000,
  DT DATE
  )
103,'TableTennis',4,8000,'14-Feb-19'
104,'Chess',2,9000,'01-Jan-19'
105,'LawnTennis',4,25000,'19-Mar-19'
106,'null',1,default,null
107,'_ _ shooting_ _',2,10000,null
DROP TABLE GAMES;
     
     
     UPDATE <TABLENAME>
     SET <COLNAME>=NEWVALUE
     WHERE <COLNAME>=<OLD VALUE>;
     
     TO REMOVE ALL TUPLES FROM  TABLE:
         DELETE FROM GAMES;
practice 
1. display all details whose eventcount is 2
select * from games where eventcount=2;

2. display all details whose gamename is carom.
select * from games where gamename='carom';

3. display all details with prizemoney exceeding 9000.
select * from games where prizemoney>9000;

1.      Display gcode, gamename and eventcount in the months: January and February or those with prizemoney below 15000.
